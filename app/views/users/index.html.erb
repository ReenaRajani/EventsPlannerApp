<% provide(:title, 'All users') %>
<h1>Listing All users</h1>

<%= will_paginate %>
<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user %>
      <%= link_to 'Show', user %></td>
      <%= link_to 'Edit', edit_user_path(user) %></td>
      <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </li>
  <% end %>
</ul>

<%= will_paginate %>