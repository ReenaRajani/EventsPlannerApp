<% provide(:title, 'Guest List') %>
<h1>Your Guest List </h1>

<ul class="guests">
  <% @guests.each do |guest| %>
    <li>
      <%= guest.name %>
      <%= guest.email_id %>
      <%= link_to 'Show', guest %></td>
      <%= link_to 'Edit', edit_guest_path(guest) %></td>
      <%= link_to 'Destroy', guest, method: :delete, data: { confirm: 'Are you sure?' } %>
    </li>
  <% end %>
</ul>
